#include<iostream>
using namespace std;

int *delElement(int *list, int *size){
    int j=0;
    
    int *k = new int[*size];
    for(int i = 1 ; i < *size ; i= i +1){
        k[j] = list[i];
        j = j +1;
    }
    *size = j;
    return k;
}

int main()
{
    int *n = new int;
    
    cout<<"size: ";
    cin>>*n;
    int *p = new int[*n];
    int *arr = new int[*n];
    
    for(int i = 0 ; i < *n ; i = i +1){
        cin>>arr[i];
    }
    
    p = delElement(arr, n);
    for(int i = 0 ; i < *n ; i = i +1){
        cout<<p[i];
    }
    
}
